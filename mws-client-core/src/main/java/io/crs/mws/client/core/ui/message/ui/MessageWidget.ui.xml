<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<ui:with field="appResources"
		type="com.gwtplatform.carstore.client.resources.AppResources" />

	<g:HTMLPanel ui:field="messageBox" styleName="{appResources.styles.message}"
		visible="false">
		<g:InlineLabel ui:field="messageLabel" />
		<g:InlineHTML ui:field="close" styleName="{appResources.styles.close}">X</g:InlineHTML>
	</g:HTMLPanel>
</ui:UiBinder> 